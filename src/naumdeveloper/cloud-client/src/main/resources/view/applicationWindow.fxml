<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="585.0" prefWidth="1166.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.haazad.cloud.client.gui.controller.ApplicationWindowController">
   <children>
      <TextField fx:id="clientPathFolder" layoutX="14.0" layoutY="22.0" onAction="#moveToClientDirectory" prefHeight="25.0" prefWidth="509.0" promptText="Client directory path" />
      <Button layoutX="523.0" layoutY="22.0" mnemonicParsing="false" onAction="#upToClientDirectory" prefHeight="25.0" prefWidth="56.0" text="Up" />
      <TableView fx:id="clientDirectoryView" layoutX="14.0" layoutY="54.0" onMouseClicked="#moveToClientDirectoryByMouseEvent" prefHeight="481.0" prefWidth="566.0" />
      <TextField fx:id="serverPathFolder" layoutX="588.0" layoutY="22.0" onAction="#moveToServerDirectory" prefHeight="25.0" prefWidth="509.0" promptText="Server directory path" />
      <Button layoutX="1097.0" layoutY="22.0" mnemonicParsing="false" onAction="#upToServerDirectory" prefHeight="25.0" prefWidth="56.0" text="Up" />
      <TableView fx:id="serverDirectoryView" layoutX="588.0" layoutY="54.0" onMouseClicked="#moveToServerDirectoryByMouseEvent" prefHeight="481.0" prefWidth="566.0" />
      <Button fx:id="uploadButton" layoutX="805.0" layoutY="541.0" mnemonicParsing="false" onAction="#uploadFileOnServer" prefHeight="25.0" prefWidth="130.0" text="Upload" />
      <Button fx:id="downloadButton" layoutX="231.0" layoutY="541.0" mnemonicParsing="false" onAction="#downloadFile" prefHeight="25.0" prefWidth="130.0" text="Download" />
   </children>
</AnchorPane>
